tshark -i wlan0mon -T fields -e frame.time_epoch -e frame.time_delta -e frame.time_delta_displayed -e frame.time_relative -e frame.len -e frame.cap_len -e radiotap.length -e radiotap.present.tsft -e radiotap.mactime -e radiotap.datarate -e radiotap.channel.freq -e wlan.fc.type_subtype -e wlan.fc.type -e wlan.fc.subtype -e wlan.fc.ds -e wlan.fc.frag -e wlan.fc.retry -e wlan.fc.pwrmgt -e wlan.fc.moredata -e wlan.fc.protected -e wlan.duration -e wlan.frag -e wlan.seq -E header=y -E separator=* > tester.csv
